<% if errors.present? %>
  <script>
    $.notify({
      icon: "tim-icons icon-simple-remove",
      message: "<b>Algo ha salido mal!</b><br /> <%= errors.full_messages.uniq.join(', ') %>",
      class: "alert-with-icon"
    }, {
      icon_type: 'class',
      type: "danger",
      delay: '0'
    });
  </script>
<% end %>
